<query id="updatePlanetUseMobile" action="update">
	<tables>
		<table name="modules" />
	</tables>
	<columns>
		<column name="use_mobile" var="use_mobile" notnull="notnull" />
	</columns>
	<conditions>
		<condition operation="equal" column="module" default="planet" />
	</conditions>
</query>
